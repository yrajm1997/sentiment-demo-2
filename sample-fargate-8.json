    "containerDefinitions": [
        {
            "name": "sentiment-cont",
            "image": "712740165373.dkr.ecr.ap-south-1.amazonaws.com/sentiment-api-demo",
            "cpu": 0,
            "portMappings": [
                {
                    "containerPort": 8001,
                    "hostPort": 8001,
